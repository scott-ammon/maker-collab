<% if (alerts.error) { %>
	<% alerts.error[0].split(',').forEach((msg) => { %>
    <div class='alert-danger'><%= msg%></div>
     <% }); %>
<% } %>

<% if (alerts.success) { %>
	<% alerts.success[0].split(',').forEach((msg) => { %>
    <div class='alert-success'><%= msg%></div>
     <% }); %>
<% } %>